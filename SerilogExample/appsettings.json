{
  
    "Serilog":{
      "Using":["Serilog.Sinks.Console","Serilog.Sinks.File"],
      "WriteTo":[
        {
          "Name":"Console"
        },
        {
          "Name":"File",
          "Args":{
            "path":"logs/myConfigLog-.txt",
            "rollingInterval":"Day"        
          }
        }
        ]
    },
  
  "AllowedHosts": "*"
}
